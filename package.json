{
  "name": "pixel-prompt-js",
  "version": "1.0.0",
  "main": "App.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "expo start",
    "start:clear": "expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "analyze": "npx expo install --fix",
    "type-check": "tsc --noEmit",
    "fix-metro": "./scripts/fix-metro.sh",
    "reset": "rm -rf node_modules && npm install && npm run start:clear"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@react-native-community/slider": "4.5.6",
    "aws-sdk": "^2.1691.0",
    "expo": "^52.0.44",
    "expo-av": "^15.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.2",
    "react-native-web": "~0.19.13",
    "zustand": "^5.0.5"
  },
  "private": true,
  "devDependencies": {
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.28.0"
  }
}
